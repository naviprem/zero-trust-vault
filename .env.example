# EKS Cluster Configuration
EKS_CLUSTER_NAME=zero-trust-cluster
AWS_VPC_ID=vpc-xxxxxxxxxxxxxxxxx
TRUST_DOMAIN=zero-trust.local

# Kubernetes Namespaces
SPIRE_NAMESPACE=spire-system
KEYCLOAK_NAMESPACE=zero-trust-infra
VAULT_NAMESPACE=zero-trust-infra

# RDS Configuration for Keycloak
RDS_HOST=your-rds-endpoint.region.rds.amazonaws.com
RDS_PORT=5432
RDS_DATABASE=keycloak
